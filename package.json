    "body-parser": "^1.17.2",
    "box-node-sdk": "^1.21.0",
    "concat": "^1.0.3",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.15.4",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "jimp": "^0.5.3",
